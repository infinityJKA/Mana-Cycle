  dialogueList:
  - text: "\u79C1\u305F\u3061\u306E\u30D2\u30FC\u30ED\u30FC\u305F\u3061\u306F\u591C\u306B\u306A\u308B\u3068\u9053\u7AEF\u306B\u7ACB\u3061\u6B62\u307E\u308A\u3001\u5C0F\u3055\u306A\u30AD\u30E3\u30F3\u30D7\u30D5\u30A1\u30A4\u30E4\u30FC\u306E\u5468\u308A\u306B\u5EA7\u3063\u3066\u30AD\u30E3\u30F3\u30D7\u3092\u3057\u307E\u3059\u3002\r\n"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 1
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u2026\r\n"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u3053\u3053\u304C\u3042\u306A\u305F\u304C\u30AD\u30E3\u30E9\u30AF\u30BF\u30FC\u3068\u3057\u3066\u30ED\u30FC\u30EB\u30D7\u30EC\u30A4\u3059\u308B\u5834\u6240\u3067\u3059\u3002"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u3042\u3042\u3002"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 11400000, guid: 808f26159863848d2be4d566709a387a, type: 2}
    rightFocused: 1
    leftAnim: 0
    rightAnim: 3
    background: {fileID: 0}
  - text: "\u3048\u3063\u3068\u2026\u300C\u65C5\u306E\u521D\u65E5\u306F\u7D42\u308F\u308A\u306B\u8FD1\u3065\u3044\u3066\u3044\u3066\u3001\u7D66\u6599\u306B\u4E00\u6B69\u8FD1\u3065\u3044\u305F\u306D\u3002\u300D"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 11400000, guid: 808f26159863848d2be4d566709a387a, type: 2}
    rightFocused: 1
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u300C\u7D66\u6599\uFF01\uFF1F\u99AC\u9E7F\u306A\u3001\u5175\u58EB\u3088\u3001\u79C1\u305F\u3061\u306F\u3053\u306E\u60AA\u3092\u6B62\u3081\u3066\u738B\u56FD\u3092\u6551\u308F\u306A\u3051\u308C\u3070\u306A\u3089\u306A\u3044\u3002\u300D"
    leftSpeaker: {fileID: 11400000, guid: 9538cdbc499e24b88ad25e76a5b5877d, type: 2}
    rightSpeaker: {fileID: 11400000, guid: 808f26159863848d2be4d566709a387a, type: 2}
    rightFocused: 0
    leftAnim: 2
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u300C\u304A\u91D1\u3082\u5927\u4E8B\u3060\u3088\u3001\u79C1\u305F\u3061\u307F\u3093\u306A\u304C\u3042\u306A\u305F\u307F\u305F\u3044\u306A\u8CB4\u65CF\u306B\u306A\u308C\u308B\u308F\u3051\u3058\u3083\u306A\u3044\u304B\u3089\u3001\u9A0E\u58EB\u3002\u300D"
    leftSpeaker: {fileID: 11400000, guid: 9538cdbc499e24b88ad25e76a5b5877d, type: 2}
    rightSpeaker: {fileID: 11400000, guid: a14066f152f1c4d2aac9d17f7def78d6, type: 2}
    rightFocused: 1
    leftAnim: 0
    rightAnim: 3
    background: {fileID: 0}
  - text: "\u300C\u541B\u3082\u3001\u6B66\u9053\u5BB6\uFF1F\u300D"
    leftSpeaker: {fileID: 11400000, guid: 9538cdbc499e24b88ad25e76a5b5877d, type: 2}
    rightSpeaker: {fileID: 11400000, guid: a14066f152f1c4d2aac9d17f7def78d6, type: 2}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u4FFA\u306F\u2026\u300C\u4FFA\u306F\u305F\u3060\u77E5\u8B58\u3092\u6C42\u3081\u3066\u308B\u3002\u3053\u306E\u738B\u56FD\u304C\u7D42\u308F\u3063\u305F\u3089\u3001\u5225\u306E\u5834\u6240\u306B\u65C5\u3059\u308B\u3060\u3051\u3055\u3002\u300D"
    leftSpeaker: {fileID: 11400000, guid: 9538cdbc499e24b88ad25e76a5b5877d, type: 2}
    rightSpeaker: {fileID: 11400000, guid: 8368327aab1454621befdb45b572f742, type: 2}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 3
    background: {fileID: 0}
  - text: "\u307F\u3093\u306A\u304C\u8B70\u8AD6\u3057\u3066\u3044\u308B\u3068\u3001\u4F55\u304B\u304C\u6697\u95C7\u304B\u3089\u5FCD\u3073\u5BC4\u3063\u3066\u304F\u308B\u3002"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 1
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u7A81\u7136\u3001\u5F7C\u3089\u306F\u7A81\u7136\u30AA\u30AA\u30AB\u30DF\u306B\u8972\u308F\u308C\u305F\uFF01"
    leftSpeaker: {fileID: 11400000, guid: 9ddcbfb1deb804e1996ac4f8143987a8, type: 2}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u6211\u3005\u306E\u82F1\u96C4\u306F\u52DD\u5229\u3092\u53CE\u3081\u308B\u306E\u3067\u3057\u3087\u3046\u304B\uFF1F"
