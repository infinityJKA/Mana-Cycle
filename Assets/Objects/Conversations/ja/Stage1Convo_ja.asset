    leftSpeaker: {fileID: 0}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "IBN\u3092\u4ECB\u3057\u3066\u63A5\u7D9A\u3059\u308B\u3053\u3068\u3067\u3001\u5C06\u6765\u306E\u9B54\u8853\u5E2B\u306F\u5B89\u5168\u306A\u74B0\u5883\u3067\u8A13\u7DF4\u3057\u3001\u4E92\u3044\u306B\u6C7A\u95D8\u3057\u305F\u308A\u3001\u5371\u967A\u306A\u30EB\u30FC\u30C1\u30F3\u3092\u5371\u967A\u3092\u5192\u3055\u305A\u306B\u8A13\u7DF4\u3057\u305F\u308A\u3059\u308B\u3053\u3068\u304C\u3067\u304D\u307E\u3059\u3002"
    leftSpeaker: {fileID: 0}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "IBN\u306F\u3059\u3050\u306B\u975E\u5E38\u306B\u4EBA\u6C17\u306B\u306A\u308A\u307E\u3057\u305F\u3002\u305D\u3057\u3066\u4ECA\u3001\u65B0\u3057\u3044\u5B66\u751F\u30B0\u30EB\u30FC\u30D7\u304C\u521D\u3081\u3066\u305D\u308C\u3092\u8A66\u3057\u3066\u3044\u307E\u3059\u2026"
    leftSpeaker: {fileID: 0}
    rightSpeaker: {fileID: 0}
    rightFocused: 0
    leftAnim: 0
    rightAnim: 0
    background: {fileID: 0}
  - text: "\u2026"
